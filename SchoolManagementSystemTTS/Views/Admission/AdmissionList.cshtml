@model IEnumerable<SchoolManagementSystemTTS.Models.Admission_vw>

@{
    ViewBag.Title = "AdmissionList";
}

 
<h4 class="text-success mt-2">@Html.Raw(TempData["success"])</h4>
<h4 class="text-danger mt-3">@Html.Raw(TempData["failed"])</h4>
<p>
    @Html.ActionLink("Create New", "AdmissionForm")
</p>
<div class="card mt-3 mb-3">
    <div class="card-header">
        <h3 class="text-center">Admission List</h3>
        <hr />
    </div>
    <div class="card-body">
        <div class="table-responsive text-nowrap">
            <table class="table table-responsive">
                <tr>
                    <th></th>
                    <th>

                        Institute
                    </th>
                    <th>
                        Campus
                    </th>
                    <th>
                        Form No.
                    </th>
                    <th>
                        Session
                    </th>
                    <th>
                        Class
                    </th>
                    <th>
                        Submitted Date
                    </th>
                    <th>
                        Date Of Birth
                    </th>
                    <th>
                        Religion
                    </th>
                    <th>
                        Domicile
                    </th>
                    <th>
                        Identification Mark
                    </th>

                    <th>
                        Address
                    </th>
                    <th>
                        Status
                    </th>
                    <th>
                        City
                    </th>
                    <th>
                        State
                    </th>
                    <th>
                        Country
                    </th>
                    <th>
                        Father Name
                    </th>

                    <th>
                        Student Cnic
                    </th>
                    <th>
                        Father Cnic
                    </th>
                    <th>
                        OCcupation
                    </th>

                    <th>
                        Phone
                    </th>
                    <th>
                        Remarks
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ADDEDBY)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ADDEDDATE)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.UPDATEDBY)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.UPDATEDATE)
                    </th>

                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.ActionLink("Edit", "updateAdmissionForm", new { id = item.Adms_ID })
                            @*@Html.ActionLink("Edit", "updateAdmissionForm", new { id = item.Adms_ID })*@
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Institute)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Campus)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FORM_NO)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.session)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Class)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SUBMITTED_DT)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DATE_OF_BIRTH)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RELIGION)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DOMICILE)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IDENTIFICATION_MARK)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ADDRESS1)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ACAD_STATUS)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Cityid)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Stateid)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Countid)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FTHR_FNAME)
                        </td>


                        <td>
                            @Html.DisplayFor(modelItem => item.STD_CNIC)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FTHR_CNIC)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OCCUPATION)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.PHONE)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.REMARKS)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.ADDEDBY)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ADDEDDATE)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UPDATEDBY)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UPDATEDATE)
                        </td>

                    </tr>
                }




            </table>
        </div>
    </div>
</div>

